@inherits ComponentBase

<MudItem>
	<MudPaper Style="height:75px;width:75px;" Outlined="true" Square="true">
		<MudButton Variant="Variant.Filled"
			Style=@GetStyle()
			Color=@GetColor()
			@onclick ="OnClick"
			>
			
			<MudText Color=@GetTextColor()>@DisplayValue</MudText>
		</MudButton>
	</MudPaper>
</MudItem>
